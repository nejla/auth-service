        location = /auth-service.js {
            proxy_pass http://authweb/auth-service.js;
        }
        location = /auth.html {
            proxy_pass http://authweb/auth.html;
            proxy_pass_request_body off;
            proxy_set_header Content-Length "";
            proxy_set_header X-Original-URI $request_uri;
            proxy_redirect http://authweb/ /;
        }
        location /api/ {
            proxy_pass http://authweb/;
            proxy_set_header X-Original-URI $request_uri;
        }
